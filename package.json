  "author": "Paul Frazee <pfrazee@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atek-cloud/coresplore/issues"
  },
  "homepage": "https://github.com/atek-cloud/coresplore#readme",
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/concat-stream": "^1.6.1",
    "@types/express": "^4.17.13",
    "@types/minimist": "^1.2.2",
    "@types/node": "^16.6.2",
    "@types/pump": "^1.1.1",
    "ava": "^3.15.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "ava": {
